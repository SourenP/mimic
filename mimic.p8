pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
player={
	x = 0,
	y = 0
}

function find_player()
	for i=0,16 do
		for j=0,16 do
			sprite_no=mget(i,j)
			if sprite_no == 3 then
				player.x = i
				player.y = j
			end
		end
	end
end

function _init()
	find_player()
end

function _update()
end

function _draw()
   cls()
   map(0,0,0,0,16,16)
   print(player.x..player.y,64,64)
end
